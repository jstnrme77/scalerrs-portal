{
  "name": "scalerrs-portal",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "cross-env NEXT_DISABLE_SWC=1 NODE_OPTIONS=\"--no-addons\" next dev",
    "build": "cross-env NEXT_DISABLE_SWC=1 NODE_OPTIONS=\"--no-addons\" next build",
